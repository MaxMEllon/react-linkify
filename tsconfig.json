{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "outDir": "./lib",
    "lib": [
      "es2020.string"
    ],
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "downlevelIteration": true,
    "importHelpers": true,
    "moduleResolution": "node"
  },
  "include": [
    "src"
  ]
}
